<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.paragonproject.ezwel.paragonproject" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".LoginActivity"
            android:label="@string/app_name"
            android:theme="@android:style/Theme.DeviceDefault" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MenuActivity"
            android:icon="@drawable/ezwel"
            android:label="@string/menuActivity_name"
            android:theme="@android:style/Theme.DeviceDefault" />
        <activity
            android:name=".VolScheduleListActivity"
            android:label="@string/scheduleActivity_name"
            android:theme="@android:style/Theme.DeviceDefault" />
        <activity
            android:name=".NMapViewer"
            android:label="@string/title_activity_nmap_viewer" >
        </activity>
    </application>

</manifest>
